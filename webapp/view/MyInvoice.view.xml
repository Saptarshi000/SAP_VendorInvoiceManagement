<mvc:View controllerName="sapvim.controller.MyInvoice"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns="sap.m"
    xmlns:unified="sap.ui.unified"
>
    <Page id="page" title="{i18n>title}">
        <content>
            <mvc:XMLView id="_IDGenXMLView1" viewName="sapvim.view.Navbar" />
            <VBox>
                <HBox class="homeVendorNameAlign">
                    <Text id="b1444" text="Vendor : " class="homeVendorName" />
                    <Text id="vendNo" text="0017300002" />
                </HBox>

                <HBox width="100%">

                    <f:Card
                        id="_IDGenCard1"
                        class="myInvcardStyles1"
                        width="100%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox2">
                                <HBox
                                    id="_IDGenHBox3"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar1"
                                        displaySize="L"
                                        backgroundColor="Accent1"
                                        displayShape="Square"
                                        src="sap-icon://batch-payments"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox4"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox1">
                                        <Link
                                            id="_IDGenText1"
                                            text="Total Invoice Submitted"
                                            press="onclickSubmitted"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency1"
                                            class="avatarStyles"
                                            stringValue="{countData>/submited}"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard2"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox22">
                                <HBox
                                    id="_IDGenHBox32"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar12"
                                        displaySize="L"
                                        backgroundColor="Accent7"
                                        displayShape="Square"
                                        src="sap-icon://accounting-document-verification"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox42"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox12">
                                        <Link
                                            id="_IDGenText1200"
                                            text="Total Invoices Verified"
                                            press="onclickverified"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency12"
                                            class="avatarStyles"
                                            stringValue="{countData>/verified}"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard13"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox23">
                                <HBox
                                    id="_IDGenHBox33"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar13"
                                        displaySize="L"
                                        backgroundColor="Accent2"
                                        displayShape="Square"
                                        src="sap-icon://alert"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox43"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox13">
                                        <Link
                                            id="_IDGenText13"
                                            text="Total Invoices Blocked"
                                            press="onclickblocked"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency13"
                                            class="avatarStyles"
                                            stringValue="{countData>/blocked}"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard14"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox24">
                                <HBox
                                    id="_IDGenHBox34"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar14"
                                        displaySize="L"
                                        backgroundColor="Accent8"
                                        displayShape="Square"
                                        src="sap-icon://payment-approval"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox44"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox14">
                                        <Link
                                            id="_IDGenText14"
                                            text="Total invoice Payments"
                                            press="onclickcleared"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency14"
                                            class="avatarStyles"
                                            stringValue="{countData>/cleared}"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>
                </HBox>

                <HBox>
                    <VBox>
                        <HBox class="filterAreaSizing">
                            <VBox id="subVbox1153"
                                class="sapUiTinyMarginBeginEnd inputPymt44 ">
                                <Text id="a2" text="Portal No."></Text>
                                <Input id="RefN" class="" width="150%" maxLength="10" />
                            </VBox>

                            <VBox id="subVbox1152"
                                class="sapUiTinyMarginBeginEnd inputPymt44 myInvFilterSec">
                                <Text id="a1" text="Invoice Date"></Text>
                                <!-- <Input id="RefNo" class="" width="150%" /> -->
                                <DatePicker id="invDate" valueFormat="yyyyMMdd" />
                            </VBox>

                            <HBox class="myInvBtnFilter myInvFilterSecBtn">
                                <Button
                                    icon="sap-icon://search"
                                    text="Search"
                                    press="onFilterInvoice"
                                />
                            </HBox>
                            <HBox class="myInvBtnFilter myInvFilterSecResetBtn">
                                <Button
                                    icon="sap-icon://reset"
                                    text="Reset"
                                    press="onclickSubmitted"
                                />
                            </HBox>
                        </HBox>
                        <FlexBox id="fbox15555" class="tabs mySamplePadding">
                            <VBox>
                                <Table
                                    alternateRowColors="true"
                                    width="auto"
                                    class="table"
                                    id="tableObj"
                                    backgroundDesign="Translucent"
                                    items="{
                                        path: 'invStatus>/results',
                                        sorter: {
                                            path: 'portal_ref'
                                        }
                                    }"
                                >

                                    <columns>
                                        <!-- <Column
                                            id="_IDGenColumn1"
                                        >
                                            <Text
                                                id="_IDGenText2"
                                                text="Vendor"
                                            />
                                        </Column> -->
                                        <Column
                                            id="_IDGenColumn132"
                                        >
                                            <Text
                                                id="_IDGenText132"
                                                text="Portal No"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn2">
                                            <Text
                                                id="_IDGenText3"
                                                text="PO number"
                                            />
                                        </Column>
                                        <Column
                                            id="_IDGenColumn3"
                                        >
                                            <Text
                                                id="_IDGenText4"
                                                text="Vendor Inv No"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn9">
                                            <Text
                                                id="_IDGenText9"
                                                text="Inv Date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn4">
                                            <Text
                                                text="Item ID" />
                                        </Column>
                                        <Column id="_IDGenColumn5"
                                            width="15%"
                                            class="header">
                                            <Text
                                                id="_IDGenText5"
                                                class="header"
                                                text="Item Description"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn6">
                                            <Text
                                                id="_IDGenText6"
                                                text="Amount"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn7">
                                            <Text
                                                id="_IDGenText7"
                                                text="Status"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn8">
                                            <Text
                                                id="_IDGenText8"
                                                text="Verified date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn11">
                                            <Text
                                                id="_IDGenText11"
                                                text="Blocked Date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn12">
                                            <Text
                                                id="_IDGenText12"
                                                text="Cleared Date"
                                            />
                                        </Column>
                                    </columns>
                                    <items>
                                        <ColumnListItem id="_IDGenColumnListItem1">
                                            <cells>
                                                <!-- <Text
                                                    text="{invStatus>vendor}"
                                                /> -->
                                                <Text
                                                    text="{invStatus>portal_ref}"
                                                />
                                                <Text
                                                    text="{invStatus>po_no}"
                                                />
                                                <Text
                                                    text="{invStatus>vendor_inv_no}"
                                                />
                                                <!-- <Text
                                                    text="{invStatus>inv_date}"
                                                /> -->
                                                <DatePicker value="{invStatus>inv_date}"
                                                    valueFormat="yyyyMMdd" editable="false" />
                                                <Text
                                                    text="{invStatus>item_ID}"
                                                />
                                                <Text
                                                    text="{invStatus>item_descp}"
                                                />
                                                <Text
                                                    text="{invStatus>amnt}"
                                                />
                                                <Text
                                                    text="{invStatus>status}"
                                                />
                                                <!-- <Text
                                                    text="{invStatus>verified_dt}"
                                                />
                                                <Text
                                                    text="{invStatus>blocked_dt}"
                                                />
                                                <Text
                                                    text="{invStatus>cleared_dt}"
                                                /> -->
                                                <DatePicker value="{invStatus>verified_dt}"
                                                    valueFormat="yyyyMMdd" editable="false" />
                                                <DatePicker value="{invStatus>blocked_dt}"
                                                    valueFormat="yyyyMMdd" editable="false" />
                                                <DatePicker value="{invStatus>cleared_dt}"
                                                    valueFormat="yyyyMMdd" editable="false" />
                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                            </VBox>
                        </FlexBox>
                    </VBox>
                </HBox>
            </VBox>
        </content>
    </Page>
</mvc:View>