<mvc:View controllerName="sapvim.controller.MyInvoice"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns="sap.m"
    xmlns:unified="sap.ui.unified"
>
    <Page id="page" title="{i18n>title}">
        <content>
            <mvc:XMLView id="_IDGenXMLView1" viewName="sapvim.view.Navbar" />
            <VBox>
                <HBox class="homeVendorNameAlign">
                    <Text id="b1444" text="Vendor : " class="homeVendorName" />
                    <Text text="S Chandra" />
                </HBox>
                
                <HBox width="100%">

                    <f:Card
                        id="_IDGenCard1"
                        class="myInvcardStyles1"
                        width="100%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox2">
                                <HBox
                                    id="_IDGenHBox3"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar1"
                                        displaySize="L"
                                        backgroundColor="Accent1"
                                        displayShape="Square"
                                        src="sap-icon://batch-payments"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox4"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox1">
                                        <Title
                                            id="_IDGenText1"
                                            text="Total Invoice Submitted"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency1"
                                            class="avatarStyles"
                                            stringValue="80"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard2"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox22">
                                <HBox
                                    id="_IDGenHBox32"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar12"
                                        displaySize="L"
                                        backgroundColor="Accent7"
                                        displayShape="Square"
                                        src="sap-icon://accounting-document-verification"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox42"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox12">
                                        <Title
                                            id="_IDGenText1200"
                                            text="Total Invoices Verified"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency12"
                                            class="avatarStyles"
                                            stringValue="80"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard13"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox23">
                                <HBox
                                    id="_IDGenHBox33"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar13"
                                        displaySize="L"
                                        backgroundColor="Accent2"
                                        displayShape="Square"
                                        src="sap-icon://alert"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox43"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox13">
                                        <Title
                                            id="_IDGenText13"
                                            text="Total Invoices Blocked"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency13"
                                            class="avatarStyles"
                                            stringValue="80"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>

                    <f:Card
                        id="_IDGenCard14"
                        class="myInvcardStyles2"
                        width="95%"
                        height="85px">
                        <f:content>
                            <HBox id="_IDGenHBox24">
                                <HBox
                                    id="_IDGenHBox34"
                                    alignItems="Start"
                                    class="avatarStyles"
                                >
                                    <Avatar
                                        id="_IDGenAvatar14"
                                        displaySize="L"
                                        backgroundColor="Accent8"
                                        displayShape="Square"
                                        src="sap-icon://payment-approval"
                                    />
                                </HBox>
                                <HBox
                                    id="_IDGenHBox44"
                                    class="detailsBoxStyles"
                                    items="80">
                                    <VBox id="_IDGenVBox14">
                                        <Title
                                            id="_IDGenText14"
                                            text="Total invoice Payments"
                                        />

                                        <unified:Currency
                                            id="_IDGenCurrency14"
                                            class="avatarStyles"
                                            stringValue="80"
                                            currency="{Currency}"
                                            useSymbol="false"
                                        />

                                    </VBox>
                                </HBox>
                            </HBox>
                        </f:content>
                    </f:Card>
                </HBox>
                <HBox>
                    <VBox>
                        <HBox class="filterAreaSizing">
                            <VBox id="subVbox1153"
                                class="sapUiTinyMarginBeginEnd inputPymt44 ">
                                <Text id="a2" text="Portal No"></Text>
                                <Input id="RefN" class="" width="150%" />
                            </VBox>

                            <VBox id="subVbox1152" class="sapUiTinyMarginBeginEnd inputPymt44 myInvFilterSec">
                                <Text id="a1" text="Invoice Date"></Text>
                                <!-- <Input id="RefNo" class="" width="150%" /> -->
                                <DatePicker id="invDate" class="" />
                            </VBox>
                            
                            <HBox class="myInvBtnFilter myInvFilterSecBtn">
                                <Button 
                                icon="sap-icon://search"
                                text="Search"
                                press="onPress"
                               />
                            </HBox>
                        </HBox>
                        <FlexBox id="fbox15555" class="tabs mySamplePadding">
                            <VBox>
                                <Table
                                    alternateRowColors="true"
                                    width="auto"
                                    class="table"
                                    id="tableObj"
                                    backgroundDesign="Translucent"
                                >
                                    <columns>
                                        <Column
                                            id="_IDGenColumn1"
                                        >
                                            <Text
                                                id="_IDGenText2"
                                                text="Vendor"
                                            />
                                        </Column>
                                        <Column
                                            id="_IDGenColumn132"
                                        >
                                            <Text
                                                id="_IDGenText132"
                                                text="Portal No"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn2">
                                            <Text
                                                id="_IDGenText3"
                                                text="PO number"
                                            />
                                        </Column>
                                        <Column
                                            id="_IDGenColumn3"
                                            width="12%"
                                            class="header">
                                            <Text
                                                id="_IDGenText4"
                                                text="Vendor Inv No"
                                                class="header"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn9">
                                            <Text
                                                id="_IDGenText9"
                                                text="Inv Date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn4">
                                            <Text
                                                text="Item ID"
                                                id="Order_Qty"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn5">
                                            <Text
                                                id="_IDGenText5"
                                                text="Item Description"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn6">
                                            <Text
                                                id="_IDGenText6"
                                                text="Amount"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn7">
                                            <Text
                                                id="_IDGenText7"
                                                text="Status"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn8">
                                            <Text
                                                id="_IDGenText8"
                                                text="Verified date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn11">
                                            <Text
                                                id="_IDGenText11"
                                                text="Blocked Date"
                                            />
                                        </Column>
                                        <Column id="_IDGenColumn12">
                                            <Text
                                                id="_IDGenText12"
                                                text="Cleared Date"
                                            />
                                        </Column>
                                    </columns>
                                </Table>
                            </VBox>
                        </FlexBox>
                    </VBox>
                </HBox>
            </VBox>
        </content>
    </Page>
</mvc:View>